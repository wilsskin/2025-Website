{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
      { "source": "/index.html", "destination": "/", "permanent": true }
    ],
    "rewrites": [
      { "source": "/:slug",  "destination": "/pages/:slug.html" },
      { "source": "/:slug/", "destination": "/pages/:slug.html" }
    ]
  }
  